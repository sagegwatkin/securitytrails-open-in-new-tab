{
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://github.com/sagegwatkin/securitytrails-open-in-new-tab",
    "author": "Sage Gwatkin",
    "version": "1.0.0",
    "manifest_version": 3,
    "default_locale": "en",
    "icons": {
        "16": "images/securitytrails-16x16.png",
        "48": "images/securitytrails-48x48.png",
        "128": "images/securitytrails-128x128.png"
    },
    "host_permissions": [
        "http://securitytrails.com/list/apex_domain/*",
        "https://securitytrails.com/list/apex_domain/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://securitytrails.com/list/apex_domain/*",
                "https://securitytrails.com/list/apex_domain/*"
            ],
            "js": [
                "open-in-new-tab.js"
            ],
            "run_at": "document_end"
        }
    ]
}